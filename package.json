{
  "name": "amap-vue-kit",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "build": "pnpm -r --filter ./packages/* run build",
    "build:all": "pnpm run build && pnpm --filter docs run build && pnpm --filter examples/basic run build",
    "dev:docs": "pnpm --filter docs run dev",
    "dev:playground": "pnpm --filter @amap-vue/playground run dev",
    "dev:example": "pnpm --filter examples/basic run dev",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "test": "vitest",
    "typecheck": "vue-tsc --build --force",
    "changeset": "changeset",
    "release": "changeset publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@changesets/cli": "^2.29.7",
    "@types/amap-js-api": "^1.4.16",
    "@types/amap-js-api-control-bar": "^1.4.0",
    "@types/amap-js-api-map-type": "^1.4.0",
    "@types/amap-js-api-scale": "^1.4.0",
    "@types/amap-js-api-tool-bar": "^1.4.0",
    "@types/node": "^24.5.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.36.0",
    "jsdom": "^27.0.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.6",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vue": "^3.5.21",
    "vue-tsc": "^3.0.7"
  }
}
